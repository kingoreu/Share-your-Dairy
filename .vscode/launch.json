{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Run ServerApplication (Spring)",
      "request": "launch",
      "mainClass": "com.share.dairy.ServerApplication",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "java",
      "name": "Run DairyYtServer (8090)",
      "request": "launch",
      "mainClass": "com.share.dairy.youtube.DairyYtServerApplication",
      "projectName": "dairy-yt-server",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "java",
      "name": "Run HelloApplication (JavaFX)",
      "request": "launch",
      "mainClass": "com.share.dairy.app.HelloApplication",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "--module-path \"C:/Java/javafx-sdk-21.0.8/lib\" --add-modules javafx.controls,javafx.fxml,javafx.web"
    },
    {
      "type": "java",
      "name": "Launcher (JavaFX)",
      "request": "launch",
      "mainClass": "com.share.dairy.app.Launcher",
      "projectName": "dairy",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "--module-path \"C:/Java/javafx-sdk-21.0.8/lib\" --add-modules javafx.controls,javafx.fxml,javafx.web"
    },
    {
      "type": "java",
      "name": "Current File (JavaFX)",
      "request": "launch",
      "mainClass": "${file}",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "--module-path \"C:/Java/javafx-sdk-21.0.8/lib\" --add-modules javafx.controls,javafx.fxml,javafx.web"
    }
  ]
}
