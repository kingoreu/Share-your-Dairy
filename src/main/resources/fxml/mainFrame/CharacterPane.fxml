<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.share.dairy.controller.character.CharacterPaneController"
            prefWidth="160" prefHeight="200">
    <children>
        <ImageView fx:id="characterImg"
                   pickOnBounds="true"
                   fitHeight="200"
                   preserveRatio="true"
                   smooth="true"
                   onMouseClicked="#onCharacterClicked"/>
    </children>

</AnchorPane>
